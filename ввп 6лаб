#include <stdio.h>
#include <math.h>
int main()
{
    printf("1 задание\n");
    float a,b,c;//с-доп.переменная
    printf("введите сначала значение а,потом б\n");
    scanf("%f%f",&a,&b);
    c=a;
    a=b;
    b=c;
    printf("a=%.2f\n",a);
    printf("b=%.2f\n\n",b);
  
    printf("2 задание\n");
    float A,B,C,D,I;//d,i-доп.переменные
    printf("введите переменные A,B,C\n");
    scanf("%f%f%f",&A,&B,&C);
    D=A;
    A=C;
    I=B;
    B=D;
    C=I;
    printf("A=%.2f\n",A);
    printf("B=%.2f\n",B); 
    printf("C=%.2f\n\n",C);
    
    printf("3 задание\n");
    float A1,B1,C1,D1,I1;//d,i-доп.переменные
    printf("введите переменные A1, B1, C1\n");
    scanf("%f%f%f",&A1,&B1,&C1);
    D1=A1;
    I1=B1;
    A1=I1;
    B1=C1;
    C1=D1;
    printf("A1=%.2f\n",A1);
    printf("B1=%.2f\n",B1); 
    printf("C1=%.2f\n\n",C1);
    
    printf("4 задание\n");
    float x,y;
    printf("Введите Х-");
    scanf("%f",&x);
    y=3*pow(x,6)-6*pow(x,2)-7;
    printf("y=%.2f\n\n",y);
    
    printf("5 задание\n");
    float X,Y;
    printf("Введите Х-");
    scanf("%f",&X);
    Y=4*pow(X-3,6)-7*pow(X-3,3)+2;
    printf("y=%.2f\n\n",Y);
    
    printf("6 задание\n");
    int p,P;
    printf("Введите число-");
    scanf("%d",&p);
    P=p*p;
    p=P*P;
    P=p*p;
    printf("P^8=%d\n\n",P);
 
    printf("7 задание\n");
    int u,r;
    long int U;//берем такой тип данных для большего диапазона значений
    printf("Введите число-");
    scanf("%d",&u);
    U=u*u;//2 
    r=u*U;//3
    r=r*U;//5
    U=r*r;//10
    U=U*r;//15
    printf("U^15=%ld\n",U);
    return 0;
}
