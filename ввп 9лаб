#include <stdio.h>

int main()
{
    printf("1 задание\n");
    int x;
    printf("Введите количество секунд,которое прошло с начала дня-");
    scanf("%d",&x);
    int z=x/60;//сколько мин прошло с начала дня
    int t=x-z*60;
    printf("Количество секунд, прошедших с начала последней минуты=%d\n\n",t);
    
    printf("2 задание\n");
    float a;
    printf("Введите число от 1 до 365-");
    scanf("%f",&a);
    int k=a/7;
    int u=k*7;
    k=a-u;
    u=k%10;
    printf("Номер для недели=%d\n\n",u);
    
    printf("3 задание\n");
    int A,B;
    printf("Введите число от 1 до 365-"); 
    scanf("%d",&A);
    printf("Введите число от 1 до 7-");
    scanf("%d",&B);
    int p=(A+B)%7-1;
    printf("Номер для недели=%d\n\n",p);
    
    printf("4 задание\n");
    int r,g,o;
    printf("Введите 1 сторону прямоугольника -"); 
    scanf("%d",&r);
    printf("Введите 2 сторону прямоугольника-");
    scanf("%d",&g);
    printf("Введите сторону квадрата-");
    scanf("%d",&o);
    int i=r*g;//площадь прямоугольника
    int s=o*o;//площадь квадрата
    int v=i/s;//количество квадратов, размещенных на прямоугольнике
    int e=v*s;//площадь занятая квадратами
    s=i-e;//площадь незанятой части прямоугольника
    printf("Количество квадратов, размещенных на прямоугольнике=%d\n\n",v);
    printf("Площадь незанятой части прямоугольника=%d\n\n",s);
    
    printf("5 задание\n");
    int god,vek;
    printf("Введите год:");
    scanf("%d", &god);
    vek = (god-1)/100 + 1;
    printf("Столетие - %d\n",vek);
  
    return 0;
}
