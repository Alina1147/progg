#include <stdio.h>
#include <math.h>
int main()
{
    printf("1 задание\n");
    printf("«Справедливы неравенства A > 2 и B ≤ 3»\n");
    int A,B;
    printf("Введите 1 число -");
    scanf("%d",&A);
    printf("Введите 2 число -");
    scanf("%d",&B);
    if(A>2 && B<=3) printf("высказывание истинно\n\n");
    else printf("высказывание не истинно\n\n");
    
    printf("2 задание\n");
    printf("«Справедливо двойное неравенство A < B < C»\n");
    int a,b,c;
    printf("Введите 1 число -");
    scanf("%d",&a);
    printf("Введите 2 число -");
    scanf("%d",&b);
    printf("Введите 3 число -");
    scanf("%d",&c);
    if(a<b && b<c) printf("высказывание истинно\n\n");
    else printf("высказывание не истинно\n\n");
    
    printf("3 задание\n");
    printf("«Данное число является четным двузначным»\n");
    int C;
    printf("Введите целое положительное число -");
    scanf("%d",&C);
    if(c%10==0 && c>0 && c<100) printf("высказывание истинно\n\n");
    else printf("высказывание не истинно\n\n");
    
    printf("4 задание\n");
    printf("Высказывание «Цифры данного числа образуют возрастающую или убывающую последовательность»\n");
    int s,s1,s2,s3;
    printf("Введите трехзначное число -");
    scanf("%d",&s);
    s1=s/100;//1 цифра
    s2=s%100/10;//2 цифра
    s3=s%10;//3 цифра
    if((s1>s2 && s2>s3) || (s1<s2 && s2<s3)) printf("высказывание истинно\n\n");
    else printf("высказывание не истинно\n\n");
    
    printf("5 задание\n");
    printf("«Данное число читается одинаково слева направо и справа налево»\n");
    int d,d1,d2,d3,d4;
    printf("Введите четырехзначное число -");
    scanf("%d",&d);
    d1=d/1000;//1 цифра
    d2=d%1000/100;//2 цифра
    d3=d%100/10;//3 цифра
    d4=d%10;//4 цифра
    if(d1==d4 && d2==d3) printf("высказывание истинно\n\n");
    else printf("высказывание не истинно\n\n");
    
    printf("6 задание\n");
    printf("«Треугольник со сторонами a, b, c является прямоугольным»\n");
    int t,y,u;
    printf("Введите длину 1 стороны -");
    scanf("%d",&t);
    printf("Введите длину 2 стороны -");
    scanf("%d",&y);
    printf("Введите длину 3 стороны -");
    scanf("%d",&u);
    if ((u>y && u>t) || (t>y && t>u) || (y>t && y>u)) {
        if((pow(t,2)==pow(y,2)+pow(u,2))||(pow(y,2)==pow(t,2)+pow(u,2))||(pow(u,2)==pow(y,2)+pow(t,2))){
            printf("высказывание истинно\n\n");}
            else printf("высказывание не истинно\n\n");
    } 
    
    printf("7 задание\n");
    printf("«Существует треугольник со сторонами a, b, c»\n");
    int h,j,k;
    printf("Введите 1 число -");
    scanf("%d",&h);
    printf("Введите 2 число -");
    scanf("%d",&j);
    printf("Введите 3 число -");
    scanf("%d",&k);
    if((h<j+k)&&j<h+k&&k<h+j) printf("высказывание истинно\n\n");
    else printf("высказывание не истинно\n\n");
    
    return 0;
}
