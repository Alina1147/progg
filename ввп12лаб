        int n,a,ans;
   printf("Введите номер года >= 1984:\n");
            scanf("%d",&n);
            if (n >= 1984) {
                printf("Год");
                n-=1983;
                a=0;
                ans = 0;
                while (a<n ) {
                    ans++;
                    a+= 12;
                }
                if (ans%5 == 0) {
                    if (n%12 == 3 || n%12 == 4 || n%12 == 5) printf(" черного");
                    else printf(" черной");
                }
                if (ans%5 == 1) {
                    if (n%12 == 3 || n%12 == 4 || n%12 == 5) printf(" зеленого");
                    else printf(" зеленой");
                }

                if (ans%5 == 2) {
                    if (n%12 == 3 || n%12 == 4 || n%12 == 5)printf(" красного");
                    else printf(" красной");
                }

                if (ans%5 == 3) {
                    if (n%12 == 3 || n%12 == 4 || n%12 == 5)  printf(" желтого");
                   else printf(" желтой");
                }
                if (ans%5 == 4) {
                    if (n%12 == 3 || n%12 == 4 || n%12 == 5) printf(" белого");
                    else printf(" белой");
                }
                if (n % 12 == 0) printf(" свиньи");
                if (n % 12 == 1) printf(" крысы");
                if (n % 12 == 2) printf(" коровы");
                if (n % 12 == 3) printf(" тигра");
                if (n % 12 == 4) printf(" зайца");
